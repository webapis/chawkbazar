// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url="mysql://root:159357@localhost:3306/products"
}


model Products {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  priceNew  String?
  imageUrl  String?
  link      String?
  timestamp String?
  marka     String?
  gender    String?
}

model Users {
  id    Int     @id @default(autoincrement())
  email String  @unique
  first_name  String?
  last_name  String?
  }

